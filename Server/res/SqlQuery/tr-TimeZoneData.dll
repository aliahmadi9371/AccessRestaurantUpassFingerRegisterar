SELECT DISTINCT
dbo.time_zones_details.time_zone_fr AS timezoneCode,
dbo.time_zones_details.start_hour AS startHour,
dbo.time_zones_details.start_minute AS startMinute,
dbo.time_zones_details.end_hour AS endHour,
dbo.time_zones_details.end_minute AS endMinute

FROM
dbo.access_group_terminals
INNER JOIN dbo.access_groups_details ON dbo.access_group_terminals.access_group_fr = dbo.access_groups_details.access_group_fr
LEFT JOIN dbo.access_times_details ON dbo.access_groups_details.access_time_fr = dbo.access_times_details.access_time_fr
LEFT JOIN dbo.time_zones_details ON dbo.access_times_details.time_zone_fr = dbo.time_zones_details.time_zone_fr
INNER JOIN dbo.terminals ON dbo.access_group_terminals.terminal_fr = dbo.terminals.id
WHERE
dbo.terminals.archived is null AND
dbo.terminals.terminalId = %1

ORDER BY dbo.time_zones_details.time_zone_fr 